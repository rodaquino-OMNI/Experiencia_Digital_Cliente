        <outputEntry id="OutputEntry_AltoEspera_2">
          <text>60</text>
        </outputEntry>
        <outputEntry id="OutputEntry_AltoEspera_3">
          <text>"NAVEGADOR_SENIOR"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_AltoEspera_4">
          <text>true</text>
        </outputEntry>
      </rule>

      <!-- Regra 6: ALTO (geral) -->
