        <outputEntry id="OutputEntry_AltoVip_1">
          <text>2</text>
        </outputEntry>
        <outputEntry id="OutputEntry_AltoVip_2">
          <text>60</text>
        </outputEntry>
        <outputEntry id="OutputEntry_AltoVip_3">
          <text>"NAVEGADOR_SENIOR"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_AltoVip_4">
          <text>true</text>
        </outputEntry>
      </rule>

      <!-- Regra 5: ALTO + >= 3 dias em espera -->
      <rule id="Rule_AltoEspera">
        <inputEntry id="InputEntry_AltoEspera_1">
          <text>"ALTO"</text>
        </inputEntry>
        <inputEntry id="InputEntry_AltoEspera_2"><text></text></inputEntry>
        <inputEntry id="InputEntry_AltoEspera_3">
