        </outputEntry>
        <outputEntry id="OutputEntry_AltoInternacao_3">
          <text>"NAVEGADOR_SENIOR"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_AltoInternacao_4">
          <text>true</text>
        </outputEntry>
      </rule>

      <!-- Regra 3: ALTO + Reclamação Aberta -->
