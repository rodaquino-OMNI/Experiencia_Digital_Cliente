        </outputEntry>
      </rule>

      <!-- Regra 4: ALTO + VIP -->
      <rule id="Rule_AltoVip">
        <inputEntry id="InputEntry_AltoVip_1">
          <text>"ALTO"</text>
        </inputEntry>
        <inputEntry id="InputEntry_AltoVip_2"><text></text></inputEntry>
        <inputEntry id="InputEntry_AltoVip_3"><text></text></inputEntry>
        <inputEntry id="InputEntry_AltoVip_4"><text></text></inputEntry>
        <inputEntry id="InputEntry_AltoVip_5"><text></text></inputEntry>
        <inputEntry id="InputEntry_AltoVip_6">
          <text>true</text>
