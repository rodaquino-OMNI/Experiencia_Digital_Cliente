      <rule id="Rule_Moderado">
        <inputEntry id="InputEntry_Moderado_1">
          <text>"MODERADO"</text>
        </inputEntry>
        <inputEntry id="InputEntry_Moderado_2"><text></text></inputEntry>
        <inputEntry id="InputEntry_Moderado_3"><text></text></inputEntry>
        <inputEntry id="InputEntry_Moderado_4"><text></text></inputEntry>
        <inputEntry id="InputEntry_Moderado_5"><text></text></inputEntry>
        <inputEntry id="InputEntry_Moderado_6"><text></text></inputEntry>
        <outputEntry id="OutputEntry_Moderado_1">
          <text>5</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Moderado_2">
          <text>480</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Moderado_3">
          <text>"NAVEGADOR_JUNIOR"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Moderado_4">
          <text>false</text>
        </outputEntry>
      </rule>

      <!-- Regra 11: BAIXO + Reclamação -->
      <rule id="Rule_BaixoReclamacao">
        <inputEntry id="InputEntry_BaixoReclamacao_1">
          <text>"BAIXO"</text>
        </inputEntry>
        <inputEntry id="InputEntry_BaixoReclamacao_2"><text></text></inputEntry>
        <inputEntry id="InputEntry_BaixoReclamacao_3"><text></text></inputEntry>
