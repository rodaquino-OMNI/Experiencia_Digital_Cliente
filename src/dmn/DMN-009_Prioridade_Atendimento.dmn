        </outputEntry>
        <outputEntry id="OutputEntry_ModeradoVip_2">
          <text>120</text>
        </outputEntry>
        <outputEntry id="OutputEntry_ModeradoVip_3">
          <text>"NAVEGADOR_PLENO"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_ModeradoVip_4">
          <text>false</text>
        </outputEntry>
      </rule>

      <!-- Regra 9: MODERADO + >= 5 dias em espera -->
      <rule id="Rule_ModeradoEspera">
        <inputEntry id="InputEntry_ModeradoEspera_1">
          <text>"MODERADO"</text>
        </inputEntry>
        <inputEntry id="InputEntry_ModeradoEspera_2"><text></text></inputEntry>
        <inputEntry id="InputEntry_ModeradoEspera_3">
          <text>&gt;= 5</text>
        </inputEntry>
        <inputEntry id="InputEntry_ModeradoEspera_4"><text></text></inputEntry>
        <inputEntry id="InputEntry_ModeradoEspera_5"><text></text></inputEntry>
        <inputEntry id="InputEntry_ModeradoEspera_6"><text></text></inputEntry>
        <outputEntry id="OutputEntry_ModeradoEspera_1">
          <text>4</text>
        </outputEntry>
        <outputEntry id="OutputEntry_ModeradoEspera_2">
          <text>240</text>
        </outputEntry>
        <outputEntry id="OutputEntry_ModeradoEspera_3">
          <text>"NAVEGADOR_PLENO"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_ModeradoEspera_4">
