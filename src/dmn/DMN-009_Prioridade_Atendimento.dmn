        <outputEntry id="OutputEntry_Alto_2">
          <text>120</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Alto_3">
          <text>"NAVEGADOR_PLENO"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Alto_4">
          <text>false</text>
        </outputEntry>
      </rule>

