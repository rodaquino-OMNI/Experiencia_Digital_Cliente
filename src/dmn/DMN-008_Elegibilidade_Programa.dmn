        <inputEntry id="InputEntry_ProgIdoso_1"><text></text></inputEntry>
        <inputEntry id="InputEntry_ProgIdoso_2">
          <text>&gt;= 65</text>
        </inputEntry>
        <inputEntry id="InputEntry_ProgIdoso_3">
