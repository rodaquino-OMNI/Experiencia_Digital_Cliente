        <outputEntry id="OutputEntry_ProgHipertensao_2">
          <text>"PROG_HIPERTENSAO"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_ProgHipertensao_3">
          <text>"LEVE"</text>
        </outputEntry>
      </rule>

