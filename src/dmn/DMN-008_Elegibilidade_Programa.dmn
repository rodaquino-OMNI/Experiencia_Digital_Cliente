      <rule id="Rule_ProgAltoRisco">
        <inputEntry id="InputEntry_ProgAltoRisco_1"><text></text></inputEntry>
        <inputEntry id="InputEntry_ProgAltoRisco_2"><text></text></inputEntry>
        <inputEntry id="InputEntry_ProgAltoRisco_3">
