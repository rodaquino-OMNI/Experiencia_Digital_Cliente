<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  id="Definitions_SUB003"
                  targetNamespace="http://bpmn.io/schema/bpmn">

  <bpmn:message id="Msg_InteracaoRecebida" name="Msg_InteracaoRecebida"/>
  <bpmn:signal id="Signal_RoteadoSelfService" name="Signal_RoteadoSelfService"/>
  <bpmn:signal id="Signal_RoteadoAgenteIA" name="Signal_RoteadoAgenteIA"/>
  <bpmn:signal id="Signal_RoteadoNavegador" name="Signal_RoteadoNavegador"/>

  <bpmn:process id="SUB-003_Recepcao_Classificacao" name="SUB-003: Recepção e Classificação Omnichannel"
                isExecutable="true" camunda:historyTimeToLive="365" camunda:versionTag="2.0">

    <bpmn:startEvent id="StartEvent_Interacao" name="Interação Recebida">
      <bpmn:outgoing>Flow_01</bpmn:outgoing>
      <bpmn:messageEventDefinition messageRef="Msg_InteracaoRecebida"/>
    </bpmn:startEvent>

    <bpmn:serviceTask id="Task_IdentificarCanal" name="Identificar Canal de Origem"
                      camunda:delegateExpression="${canalService.identificar}">
      <bpmn:incoming>Flow_01</bpmn:incoming>
      <bpmn:outgoing>Flow_02</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_BuscarBeneficiario" name="Buscar Beneficiário"
                      camunda:delegateExpression="${tasyBeneficiarioService.buscar}">
      <bpmn:extensionElements>
        <camunda:failedJobRetryTimeCycle>R3/PT1M</camunda:failedJobRetryTimeCycle>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_02</bpmn:incoming>
      <bpmn:outgoing>Flow_03</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_EnriquecerContexto" name="Enriquecer Contexto 360°"
                      camunda:delegateExpression="${contextoService.enriquecer360}">
      <bpmn:incoming>Flow_03</bpmn:incoming>
      <bpmn:outgoing>Flow_04</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_ClassificarIntencao" name="Classificar Intenção (NLP)"
                      camunda:type="external"
                      camunda:topic="nlp-classificacao">
      <bpmn:extensionElements>
        <camunda:failedJobRetryTimeCycle>R3/PT30S</camunda:failedJobRetryTimeCycle>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_04</bpmn:incoming>
      <bpmn:outgoing>Flow_05</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:businessRuleTask id="Task_ClassificarUrgencia" name="Classificar Urgência"
                           camunda:resultVariable="nivelUrgencia"
                           camunda:decisionRef="DMN_ClassificarUrgencia">
      <bpmn:incoming>Flow_05</bpmn:incoming>
      <bpmn:outgoing>Flow_06</bpmn:outgoing>
    </bpmn:businessRuleTask>

    <bpmn:businessRuleTask id="Task_DefinirRoteamento" name="Definir Roteamento"
                           camunda:resultVariable="camadaDestino"
                           camunda:decisionRef="DMN_DefinirRoteamento">
      <bpmn:incoming>Flow_06</bpmn:incoming>
      <bpmn:outgoing>Flow_07</bpmn:outgoing>
    </bpmn:businessRuleTask>

    <bpmn:exclusiveGateway id="Gateway_Roteamento" name="Camada Destino?">
      <bpmn:incoming>Flow_07</bpmn:incoming>
      <bpmn:outgoing>Flow_08_SelfService</bpmn:outgoing>
      <bpmn:outgoing>Flow_09_AgenteIA</bpmn:outgoing>
      <bpmn:outgoing>Flow_10_Navegador</bpmn:outgoing>
      <bpmn:outgoing>Flow_11_Reclamacao</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:callActivity id="CallActivity_SUB004" name="SUB-004: Self-Service"
                       calledElement="SUB-004_Self_Service">
      <bpmn:extensionElements>
        <camunda:in source="beneficiarioId" target="beneficiarioId"/>
        <camunda:in source="demandaContext" target="demandaContext"/>
        <camunda:out source="demandaResolvida" target="demandaResolvida"/>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_08_SelfService</bpmn:incoming>
      <bpmn:outgoing>Flow_12</bpmn:outgoing>
    </bpmn:callActivity>

    <bpmn:callActivity id="CallActivity_SUB005" name="SUB-005: Agente IA"
                       calledElement="SUB-005_Agentes_IA">
      <bpmn:extensionElements>
        <camunda:in source="beneficiarioId" target="beneficiarioId"/>
        <camunda:in source="demandaContext" target="demandaContext"/>
        <camunda:out source="demandaResolvida" target="demandaResolvida"/>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_09_AgenteIA</bpmn:incoming>
      <bpmn:outgoing>Flow_13</bpmn:outgoing>
    </bpmn:callActivity>

    <bpmn:callActivity id="CallActivity_SUB007" name="SUB-007: Navegação"
                       calledElement="SUB-007_Navegacao_Cuidado">
      <bpmn:extensionElements>
        <camunda:in source="beneficiarioId" target="beneficiarioId"/>
        <camunda:in source="demandaContext" target="demandaContext"/>
        <camunda:out source="jornadaId" target="jornadaId"/>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_10_Navegador</bpmn:incoming>
      <bpmn:outgoing>Flow_14</bpmn:outgoing>
    </bpmn:callActivity>

    <bpmn:callActivity id="CallActivity_SUB009" name="SUB-009: Reclamações"
                       calledElement="SUB-009_Gestao_Reclamacoes">
      <bpmn:extensionElements>
        <camunda:in source="beneficiarioId" target="beneficiarioId"/>
        <camunda:in source="demandaContext" target="demandaContext"/>
        <camunda:out source="reclamacaoId" target="reclamacaoId"/>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_11_Reclamacao</bpmn:incoming>
      <bpmn:outgoing>Flow_15</bpmn:outgoing>
    </bpmn:callActivity>

    <bpmn:serviceTask id="Task_RegistrarAtendimento" name="Registrar Atendimento"
                      camunda:delegateExpression="${atendimentoService.registrar}">
      <bpmn:incoming>Flow_12</bpmn:incoming>
      <bpmn:incoming>Flow_13</bpmn:incoming>
      <bpmn:incoming>Flow_14</bpmn:incoming>
      <bpmn:incoming>Flow_15</bpmn:incoming>
      <bpmn:outgoing>Flow_16</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:sendTask id="Task_EnviarConfirmacao" name="Enviar Confirmação"
                   camunda:type="external"
                   camunda:topic="whatsapp-sender">
      <bpmn:incoming>Flow_16</bpmn:incoming>
      <bpmn:outgoing>Flow_17</bpmn:outgoing>
    </bpmn:sendTask>

    <bpmn:endEvent id="EndEvent_InteracaoProcessada" name="Interação Processada">
      <bpmn:incoming>Flow_17</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_01" sourceRef="StartEvent_Interacao" targetRef="Task_IdentificarCanal"/>
    <bpmn:sequenceFlow id="Flow_02" sourceRef="Task_IdentificarCanal" targetRef="Task_BuscarBeneficiario"/>
    <bpmn:sequenceFlow id="Flow_03" sourceRef="Task_BuscarBeneficiario" targetRef="Task_EnriquecerContexto"/>
    <bpmn:sequenceFlow id="Flow_04" sourceRef="Task_EnriquecerContexto" targetRef="Task_ClassificarIntencao"/>
    <bpmn:sequenceFlow id="Flow_05" sourceRef="Task_ClassificarIntencao" targetRef="Task_ClassificarUrgencia"/>
    <bpmn:sequenceFlow id="Flow_06" sourceRef="Task_ClassificarUrgencia" targetRef="Task_DefinirRoteamento"/>
    <bpmn:sequenceFlow id="Flow_07" sourceRef="Task_DefinirRoteamento" targetRef="Gateway_Roteamento"/>
    <bpmn:sequenceFlow id="Flow_08_SelfService" name="Self-Service" sourceRef="Gateway_Roteamento" targetRef="CallActivity_SUB004">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${camadaDestino == 'SELF_SERVICE'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_09_AgenteIA" name="Agente IA" sourceRef="Gateway_Roteamento" targetRef="CallActivity_SUB005">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${camadaDestino == 'AGENTE_IA'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_10_Navegador" name="Navegador" sourceRef="Gateway_Roteamento" targetRef="CallActivity_SUB007">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${camadaDestino == 'NAVEGADOR'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_11_Reclamacao" name="Reclamação" sourceRef="Gateway_Roteamento" targetRef="CallActivity_SUB009">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${camadaDestino == 'RECLAMACAO'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_12" sourceRef="CallActivity_SUB004" targetRef="Task_RegistrarAtendimento"/>
    <bpmn:sequenceFlow id="Flow_13" sourceRef="CallActivity_SUB005" targetRef="Task_RegistrarAtendimento"/>
    <bpmn:sequenceFlow id="Flow_14" sourceRef="CallActivity_SUB007" targetRef="Task_RegistrarAtendimento"/>
    <bpmn:sequenceFlow id="Flow_15" sourceRef="CallActivity_SUB009" targetRef="Task_RegistrarAtendimento"/>
    <bpmn:sequenceFlow id="Flow_16" sourceRef="Task_RegistrarAtendimento" targetRef="Task_EnviarConfirmacao"/>
    <bpmn:sequenceFlow id="Flow_17" sourceRef="Task_EnviarConfirmacao" targetRef="EndEvent_InteracaoProcessada"/>

  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_SUB003">
    <bpmndi:BPMNPlane id="BPMNPlane_SUB003" bpmnElement="SUB-003_Recepcao_Classificacao">

      <!-- Start Event -->
      <bpmndi:BPMNShape id="StartEvent_Interacao_di" bpmnElement="StartEvent_Interacao">
        <dc:Bounds x="172" y="102" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <!-- Service Tasks -->
      <bpmndi:BPMNShape id="Task_IdentificarCanal_di" bpmnElement="Task_IdentificarCanal">
        <dc:Bounds x="280" y="80" width="100" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_BuscarBeneficiario_di" bpmnElement="Task_BuscarBeneficiario">
        <dc:Bounds x="440" y="80" width="100" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_EnriquecerContexto_di" bpmnElement="Task_EnriquecerContexto">
        <dc:Bounds x="600" y="80" width="100" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_ClassificarIntencao_di" bpmnElement="Task_ClassificarIntencao">
        <dc:Bounds x="760" y="80" width="100" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_ClassificarUrgencia_di" bpmnElement="Task_ClassificarUrgencia">
        <dc:Bounds x="920" y="80" width="100" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_DefinirRoteamento_di" bpmnElement="Task_DefinirRoteamento">
        <dc:Bounds x="1080" y="80" width="100" height="80"/>
      </bpmndi:BPMNShape>

      <!-- Gateway -->
      <bpmndi:BPMNShape id="Gateway_Roteamento_di" bpmnElement="Gateway_Roteamento">
        <dc:Bounds x="1245" y="95" width="50" height="50"/>
      </bpmndi:BPMNShape>

      <!-- Call Activities -->
      <bpmndi:BPMNShape id="CallActivity_SUB004_di" bpmnElement="CallActivity_SUB004">
        <dc:Bounds x="1380" y="30" width="130" height="90"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="CallActivity_SUB005_di" bpmnElement="CallActivity_SUB005">
        <dc:Bounds x="1380" y="150" width="130" height="90"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="CallActivity_SUB007_di" bpmnElement="CallActivity_SUB007">
        <dc:Bounds x="1380" y="270" width="130" height="90"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="CallActivity_SUB009_di" bpmnElement="CallActivity_SUB009">
        <dc:Bounds x="1380" y="390" width="130" height="90"/>
      </bpmndi:BPMNShape>

      <!-- Final Service Tasks -->
      <bpmndi:BPMNShape id="Task_RegistrarAtendimento_di" bpmnElement="Task_RegistrarAtendimento">
        <dc:Bounds x="1580" y="200" width="100" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_EnviarConfirmacao_di" bpmnElement="Task_EnviarConfirmacao">
        <dc:Bounds x="1740" y="200" width="100" height="80"/>
      </bpmndi:BPMNShape>

      <!-- End Event -->
      <bpmndi:BPMNShape id="EndEvent_InteracaoProcessada_di" bpmnElement="EndEvent_InteracaoProcessada">
        <dc:Bounds x="1902" y="222" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <!-- Sequence Flows (Edges) -->
      <bpmndi:BPMNEdge id="Flow_01_di" bpmnElement="Flow_01">
        <di:waypoint x="208" y="120"/>
        <di:waypoint x="280" y="120"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_02_di" bpmnElement="Flow_02">
        <di:waypoint x="380" y="120"/>
        <di:waypoint x="440" y="120"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_03_di" bpmnElement="Flow_03">
        <di:waypoint x="540" y="120"/>
        <di:waypoint x="600" y="120"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_04_di" bpmnElement="Flow_04">
        <di:waypoint x="700" y="120"/>
        <di:waypoint x="760" y="120"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_05_di" bpmnElement="Flow_05">
        <di:waypoint x="860" y="120"/>
        <di:waypoint x="920" y="120"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_06_di" bpmnElement="Flow_06">
        <di:waypoint x="1020" y="120"/>
        <di:waypoint x="1080" y="120"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_07_di" bpmnElement="Flow_07">
        <di:waypoint x="1180" y="120"/>
        <di:waypoint x="1245" y="120"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_08_SelfService_di" bpmnElement="Flow_08_SelfService">
        <di:waypoint x="1270" y="95"/>
        <di:waypoint x="1270" y="75"/>
        <di:waypoint x="1380" y="75"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_09_AgenteIA_di" bpmnElement="Flow_09_AgenteIA">
        <di:waypoint x="1270" y="120"/>
        <di:waypoint x="1270" y="195"/>
        <di:waypoint x="1380" y="195"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_10_Navegador_di" bpmnElement="Flow_10_Navegador">
        <di:waypoint x="1270" y="145"/>
        <di:waypoint x="1270" y="315"/>
        <di:waypoint x="1380" y="315"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_11_Reclamacao_di" bpmnElement="Flow_11_Reclamacao">
        <di:waypoint x="1295" y="120"/>
        <di:waypoint x="1335" y="120"/>
        <di:waypoint x="1335" y="435"/>
        <di:waypoint x="1380" y="435"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_12_di" bpmnElement="Flow_12">
        <di:waypoint x="1510" y="75"/>
        <di:waypoint x="1630" y="75"/>
        <di:waypoint x="1630" y="200"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_13_di" bpmnElement="Flow_13">
        <di:waypoint x="1510" y="195"/>
        <di:waypoint x="1545" y="195"/>
        <di:waypoint x="1545" y="240"/>
        <di:waypoint x="1580" y="240"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_14_di" bpmnElement="Flow_14">
        <di:waypoint x="1510" y="315"/>
        <di:waypoint x="1545" y="315"/>
        <di:waypoint x="1545" y="240"/>
        <di:waypoint x="1580" y="240"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_15_di" bpmnElement="Flow_15">
        <di:waypoint x="1510" y="435"/>
        <di:waypoint x="1630" y="435"/>
        <di:waypoint x="1630" y="280"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_16_di" bpmnElement="Flow_16">
        <di:waypoint x="1680" y="240"/>
        <di:waypoint x="1740" y="240"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_17_di" bpmnElement="Flow_17">
        <di:waypoint x="1840" y="240"/>
        <di:waypoint x="1902" y="240"/>
      </bpmndi:BPMNEdge>

    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>

</bpmn:definitions>
